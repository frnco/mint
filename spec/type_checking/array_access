component Main {
  fun test : Maybe(String) {
    [
      "Hello",
      "Blah",
      "Joe"
    ][1]
  }

  fun render : String {
    ""
  }
}
-------------------------------------------------------ArrayAccessIndexNotNumber
component Main {
  fun test : Maybe(String) {
    [
      "Hello",
      "Blah",
      "Joe"
    ]["asd"]
  }

  fun render : Html {
    test()

    <div/>
  }
}
---------------------------------------------------------ArrayAccessInvalidTuple
component Main {
  fun test : Maybe(String) {
    {}[0]
  }

  fun render : Html {
    test()

    <div/>
  }
}
--------------------------------------------------------------------------------
component Main {
  fun test : String {
    {"Hello"}[0]
  }

  fun render : Html {
    test()

    <div/>
  }
}
---------------------------------------------------------ArrayAccessInvalidTuple
component Main {
  fun test : Maybe(String) {
    {"Hello"}[1]
  }

  fun render : Html {
    test()

    <div/>
  }
}
