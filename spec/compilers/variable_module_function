module A {
  fun a : String {
    "Hello"
  }

  fun b : Function(String) {
    a
  }
}

component Main {
  fun render : String {
    a =
      A.b()

    a()
  }
}
--------------------------------------------------------------------------------
const B = new(class extends _M {
  c() {
    return `Hello`;
  }

  b() {
    return B.c;
  }
});

class A extends _C {
  render() {
    const a = B.b();
    return a();
  }
};

A.displayName = "Main";

