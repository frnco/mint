record X {
  name : String
}

component Main {
  fun render : String {
    x =
      { name = "test" }

    x.name
  }
}
--------------------------------------------------------------------------------
const A = _R({
  name: [
    "name",
    Decoder.string
  ]
});

class B extends _C {
  render() {
    const a = new A({
      name: `test`
    });

    return a.name;
  }
};

B.displayName = "Main";
