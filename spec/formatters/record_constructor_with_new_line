record Test {
  name : String,
  age : Number
}

component Main {
  fun render : String {
    item =
      Test("Joe",
        32)

    item.name
  }
}
--------------------------------------------------------------------------------
record Test {
  name : String,
  age : Number
}

component Main {
  fun render : String {
    item =
      Test(
        "Joe",
        32)

    item.name
  }
}
