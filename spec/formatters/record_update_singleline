record A {
  a : String
}

module Test {
  fun test : A {
    x =
      { a = "Blah" }

    {x|a="Hello"}
  }
}
--------------------------------------------------------------------------------
record A {
  a : String
}

module Test {
  fun test : A {
    x =
      { a = "Blah" }

    { x | a = "Hello" }
  }
}
